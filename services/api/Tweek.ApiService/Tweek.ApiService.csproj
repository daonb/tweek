    <ProjectReference Include="..\..\..\core\Engine\Tweek.Engine.Core\Tweek.Engine.Core.csproj" />
    <ProjectReference Include="..\..\..\core\Engine\Tweek.Engine.DataTypes\Tweek.Engine.DataTypes.csproj" />
