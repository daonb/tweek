{
  "name": "tweek-editor",
  "version": "0.2.10",
  "main": "dist/index.js",
  "repository": "Soluto/tweek",
  "author": "Soluto",
  "license": "MIT",
  "engines": {
    "node": ">=8.7.0"
  },
  "scripts": {
    "start:service-worker": "webpack --config ./service-worker/webpack.config.js --watch",
    "start:client": "yarn build-less && concurrently --kill-others \"yarn watch-less\" \"react-scripts start\"",
    "start:server": "nodemon --watch server server/index.js --exec babel-node -- --GIT_USER git --GIT_URL ssh://git@localhost:4001/tweek/repo --GIT_PUBLIC_KEY_PATH ../../deployments/dev/ssh/tweekgit.pub --GIT_PRIVATE_KEY_PATH ../../deployments/dev/ssh/tweekgit --TWEEK_API_HOSTNAME http://localhost:4003 --AUTHORING_API_HOSTNAME http://localhost:4005 --MANAGEMENT_HOSTNAME=http://localhost:4002",
    "start": "concurrently --kill-others \"yarn start:service-worker\" \"yarn start:client\" \"yarn start:server\"",
    "start:full-env": "yarn docker-compose up -d api authoring management && yarn start",
    "start:release": "node ./dist/index.js",
    "build:service-worker": "webpack --config ./service-worker/webpack.config.js",
    "build:client": "yarn build-less && react-scripts build",
    "build:server": "babel server --out-dir dist && exit 0",
    "build": "concurrently --kill-others-on-fail \"yarn build:service-worker && yarn build:client\" \"yarn build:server\"",
    "test": "jest",
    "eject": "react-scripts eject",
    "teardown": "yarn docker-compose down --remove-orphans",
    "logs": "yarn docker-compose logs",
    "docker-compose": "docker-compose -f ../../deployments/dev/docker-compose.yml -f ../../deployments/dev/docker-compose.override.yml",
    "build-less": "node-less-chokidar src/",
    "watch-less": "yarn build-less && node-less-chokidar src/ -o src/ --watch --recursive"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:3001",
      "secure": false
    },
    "/login": {
      "target": "http://localhost:3001",
      "secure": false
    },
    "/auth": {
      "target": "http://localhost:3001",
      "secure": false
    }
  },
  "jest": {
    "testRegex": "/spec/unit/.*\\.js$",
    "coverageDirectory": "./coverage-unit/",
    "collectCoverage": true
  },
  "dependencies": {
    "animakit-expander": "^2.1.3",
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "chance": "^1.0.12",
    "change-case": "^3.0.1",
    "classnames": "^2.2.5",
    "crypto": "^1.0.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "fast-json-patch": "^2.0.6",
    "helmet": "^3.9.0",
    "history": "^4.7.2",
    "idb-keyval": "^2.3.0",
    "jsonwebtoken": "^8.1.0",
    "lunr": "^2.1.4",
    "moment": "^2.19.2",
    "morgan": "^1.9.0",
    "nconf": "^0.9.1",
    "papp-polyfill": "^1.1.0",
    "passport": "^0.4.0",
    "passport-azure-ad": "^3.0.6",
    "passport-google-oauth20": "^1.0.0",
    "passport-http": "^0.3.0",
    "passport-oauth2": "^1.4.0",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.1.1",
    "react-autosize-textarea": "^0.4.9",
    "react-dom": "^16.1.1",
    "react-draggable": "^3.0.3",
    "react-google-tag-manager": "^2.2.1",
    "react-highlight-words": "^0.10.0",
    "react-monaco-editor": "^0.12.0",
    "react-notification-system": "^0.2.16",
    "react-redux": "^5.0.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-sanfona": "^1.0.5",
    "react-tabs": "^2.1.0",
    "react-tag-input": "^4.7.2",
    "react-tap-event-plugin": "^3.0.2",
    "react-tooltip": "^3.4.0",
    "react-virtualized": "^9.12.0",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "rodal": "^1.6.1",
    "rxjs": "^5.5.2",
    "velocity-react": "^1.3.3",
    "web-push": "^3.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-things": "^0.2.0",
    "concurrently": "^3.4.0",
    "eslint": "^4.11.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "fetch-mock": "^5.13.1",
    "jest-cli": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "node-less-chokidar": "^0.1.2",
    "nodemon": "^1.12.1",
    "react-scripts": "1.0.14",
    "react-test-renderer": "^16.1.1",
    "rimraf": "^2.6.2"
  }
}
